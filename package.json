  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "dev-cli": "quasar dev -m electron -- --cli=true",
    "dev-elec": "quasar dev -m electron -- test --test2=2 test3=2 --testo4",
    "dev-elect": "quasar dev -m electron --devtools",
    "build-elec": "quasar build --mode electron --publish always"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "highlight.js": "^11.8.0",
    "marked": "^5.1.1",
    "marked-highlight": "^2.0.1",
    "openai": "^4.0.0-beta.4",
    "pinia": "^2.0.11",
    "prismjs": "^1.29.0",
    "quasar": "^2.6.0",
    "vue": "^3.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@types/node": "^12.20.21",
